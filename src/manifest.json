{
  "manifestVersion": 5,
  "name": "Easy-Design-Handoff",
  "main": "main.js",
  "host": {
    "app": "XD",
    "minVersion": "21.0"
  },
  "id": "1f25036c",

  "entrypoints": [
    {
      "type": "command",

      "label": {
        "default": "Export all artboards"
      },
      "id": "exportAllArtboards"
    },
    {
      "type": "command",

      "label": {
        "default": "Export current artboard"
      },
      "id": "exportCurrentArtboard"
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "fullAccess",
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": ["http", "https"],
      "extensions": [".svg", ".png", ".js"]
    }
  },
  "icons": [
    {
      "path": "images/icon@1x.png",
      "width": 24,
      "height": 24
    },
    {
      "path": "images/icon@2x.png",
      "width": 48,
      "height": 48
    }
  ],
  "version": "1.0.0"
}
